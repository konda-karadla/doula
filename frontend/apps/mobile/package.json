{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "start": "cross-env EXPO_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --offline",
    "start:clean": "cross-env EXPO_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --clear --offline",
    "start:tunnel": "cross-env EXPO_NO_DOCTOR=1 expo start --tunnel",
    "android": "cross-env EXPO_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --android --offline",
    "ios": "cross-env EXPO_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --ios --offline",
    "web": "expo start --web",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "expo": "~54.0.12",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0",
    "@health-platform/types": "*",
    "@health-platform/api-client": "*",
    "@health-platform/utils": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2",
    "cross-env": "^7.0.3"
  }
}
