{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "start": "cross-env EXPO_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --offline",
    "start:clean": "cross-env EXPO_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --clear --offline",
    "start:tunnel": "cross-env EXPO_NO_DOCTOR=1 expo start --tunnel",
    "android": "cross-env EXPO_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --android --offline",
    "ios": "cross-env EXPO_NO_DOCTOR=1 EXPO_OFFLINE=1 expo start --ios --offline",
    "web": "expo start --web",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@health-platform/api-client": "*",
    "@health-platform/types": "*",
    "@health-platform/utils": "*",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0",
    "expo": "~54.0.12",
    "expo-constants": "^18.0.9",
    "expo-haptics": "^15.0.7",
    "expo-linking": "^8.0.8",
    "expo-local-authentication": "^17.0.7",
    "expo-router": "^6.0.12",
    "expo-secure-store": "^15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-status-bar": "~3.0.8",
    "lucide-react-native": "^0.545.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/babel-preset": "^0.81.4",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.1.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|expo|@expo|@react-navigation|zustand)/)"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/metro.config.js",
      "!**/.expo/**",
      "!**/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node"
  }
}
